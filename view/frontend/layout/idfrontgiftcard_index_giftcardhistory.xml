<?xml version="1.0" ?>
<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <update handle="customer_account" />
    <body>
        <referenceBlock name="page.main.title">
            <action method="setPageTitle">
                <argument name="title" xsi:type="string" translate="true">My Gift Card</argument>
            </action>
        </referenceBlock>
        <referenceContainer name="content">
            <block class="Mageplaza\GiftCard\Block\GiftCardHistory" name="giftcard_history_redeem" template="Mageplaza_GiftCard::giftcard_history.phtml" cacheable="false"/>
        </referenceContainer>
    </body>
</page>